{
  "name": "krycylab",
  "version": "1.0.0",
  "description": "Secure Cat Management app with Prisma, pglite, and Nunjucks.",
  "main": "dist/src/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:lan": "APP_HOST=0.0.0.0 tsx watch src/server.ts",
    "dev:lan:ps": "powershell -NoProfile -Command \"$env:APP_HOST='0.0.0.0'; tsx watch src/server.ts\"",
    "dev:full": "concurrently \"pnpm dev\" \"pnpm tailwind\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "start:lan": "APP_HOST=0.0.0.0 node dist/src/server.js",
    "start:lan:ps": "powershell -NoProfile -Command \"$env:APP_HOST='0.0.0.0'; node dist/src/server.js\"",
    "lint": "tsc --noEmit",
    "postinstall": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "seed": "tsx prisma/seed.ts",
    "tailwind": "tailwindcss -i ./src/styles/tailwind.css -o ./public/assets/tailwind.css --watch",
    "tailwind:build": "tailwindcss -i ./src/styles/tailwind.css -o ./public/assets/tailwind.css",
    "test": "echo \"No automated tests configured\" && exit 0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "@electric-sql/pglite": "^0.3.14",
    "@prisma/client": "^6.19.0",
    "autoprefixer": "^10.4.22",
    "bcryptjs": "^3.0.3",
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "nunjucks": "^3.2.4",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "prisma": "^6.19.0",
    "qrcode": "^1.5.4",
    "session-file-store": "^1.5.0",
    "speakeasy": "^2.0.0",
    "tailwindcss": "^3.4.18",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/express-session": "^1.18.2",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/nunjucks": "^3.2.6",
    "@types/passport": "^1.0.17",
    "@types/passport-local": "^1.0.38",
    "@types/qrcode": "^1.5.6",
    "@types/session-file-store": "^1.2.6",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^11.0.0",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
